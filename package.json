{
	"name": "vscode-tour-of-go",
	"displayName": "VS Code Tour of Go",
	"description": "A tour of the Go programming language via VS Code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"author": {
        "name": "Aaaaash",
        "email": "binshao54@gmail.com"
	},
	"publisher": "aaaaash",
	"bugs": {
        "url": "https://github.com/Aaaaash/vscode-tour-of-go/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Aaaaash/vscode-tour-of-go.git"
	},
	"activationEvents": [
		"onCommand:tourOfGo-launch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tourOfGo-launch",
				"title": "Launch Tour of Go"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install && cd webview && yarn install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
